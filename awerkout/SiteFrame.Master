<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="SiteFrame.Master.cs" Inherits="awerkout.Site1" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <link rel="stylesheet" href="mainstyle.css" />
</head>
<body>
    <form id="form1" runat="server">
        <div>
            <h1 style="text-align: center;">Awerkout - An awesome workout site</h1>
            <div >
                <table style="width: 100%;">
                    <tr>
                        <td>
                            <asp:LinkButton ID="homelnk" runat="server" class="nav" OnClick="homelnk_Click" CausesValidation="False">AWERKOUT</asp:LinkButton>
                            <asp:LinkButton ID="planslnk" runat="server" class="nav" OnClick="planslnk_Click" CausesValidation="False">WORKOUT PLANS</asp:LinkButton>
